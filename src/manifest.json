{
  "manifest_version": 3,
  "name": "move2parent",
  "version": "2.0.0",
  "default_locale": "ja",
  "description": "__MSG_extDes__",
  "icons": {
    "16": "img/icon_016.png",
    "32": "img/icon_032.png",
    "48": "img/icon_048.png",
    "64": "img/icon_064.png",
    "128": "img/icon_128.png"
  },
  "permissions": [
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "optional_host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_icon": {
      "32": "img/icon_032.png",
      "64": "img/icon_064.png"
    },
    "default_title": "move to parent"
  },
  "background": {
    "service_worker": "js/service-worker.js",
    "type": "module"
  }
}